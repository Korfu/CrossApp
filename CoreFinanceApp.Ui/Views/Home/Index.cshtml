
@{
    ViewBag.Title = "Index";
}

<h4>Please select a file to upload</h4>
<hr />
@using (Html.BeginForm("UploadExcel", "Home", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return myFunction()" }))
{

    <div class="form-group">

        <div class="control-label col-md-2">Excel type file (.xlsx):</div>
        <div class="col-md-10">
            <input type="file" id="FileUpload" name="FileUpload" class="" />
        </div>
    </div>
    <div class="alert-info">
        @ViewBag.ExcelUploadMessage
    </div>
    <div class="form-group">
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <div></div>
                <input type="submit" value="Upload" id="btnSubmit" class="btn btn-primary" />

            </div>
        </div>

        }
